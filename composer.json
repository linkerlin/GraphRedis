{
    "name": "linkerlin/graph-redis",
    "description": "A Graph Database with Redis!",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": ["graph", "database", "redis", "graph-database"],
    "authors": [
        {
            "name": "linkerlin",
            "email": "linkerlin@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "GraphRedis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphRedis\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}